.list{
    width:100%;
	display:block;
    /* height:70px; */
    position:relative;
    border-bottom:1rpx solid #f0f0f0;
}
.list .left{
    width:100%;
    background:#fff;
}

.list .right{
   overflow: hidden;
    position: absolute;
    left:100%;
	height:100%;
    top:0;
    color:#fff;
    line-height:70px;
    text-align:center;
	z-index:9;
}
.list .right view{
	display:flex;
    height:100%;
    float:left;
	justify-content:center;
	align-items: center;
	font-size:24rpx;
}
